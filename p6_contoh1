#include <iostream> // Define iostream
using namespace std; // menggunakan std
int main()
{
     int n;
    cout<<"Masukan jumlah yang diinginkan :";
    cin>>n;

    for(int i=0; i<=n; i++){
        for(int j=0; j<i; j++){
            cout<<"*";
            }
            cout<<endl;
    }
    return 0;
}
